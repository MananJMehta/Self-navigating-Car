<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_google_signin_btn_text_dark_pressed"
    tools:context="sjsucmpe243.application.tatanano.MapsActivity">

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="1.0"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toTopOf="parent"
        map:layout_constraintVertical_bias="1.0"
        tools:context="sjsucmpe243.application.tatanano.MapsActivity"
        tools:ignore="MissingConstraints" />

    <!--    <android.support.design.widget.FloatingActionButton
        android:id="@+id/bluetooth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:src="@drawable/ic_bluetooth"
        app:backgroundTint="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintStart_toStartOf="parent" />-->

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/SendLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="32dp"
        android:src="@drawable/ic_send_locations"
        app:backgroundTint="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SendStart"
        app:layout_constraintVertical_bias="0.0" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/SendStart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="280dp"
        android:src="@drawable/ic_send_start"
        app:backgroundTint="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/SendStop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="280dp"
        android:src="@drawable/ic_send_stop"
        android:visibility="invisible"
        app:backgroundTint="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/compass"
        android:layout_width="329dp"
        android:layout_height="176dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/autoinfo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/speedText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="SpeedVal"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/speedLable"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.133" />

    <TextView
        android:id="@+id/CurrPosText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="LocationVal"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/CurrPosLable"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.246" />

    <TextView
        android:id="@+id/HeadingText"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="HeadingVal"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/HeadingLable"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.363" />

    <TextView
        android:id="@+id/bearingText"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="BearingVal"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toEndOf="@+id/bearingLable"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.482" />

    <TextView
        android:id="@+id/deflectionAngle"
        android:layout_width="77dp"
        android:layout_height="17dp"
        android:layout_marginBottom="60dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="DeflectionVal"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/deflectionAngleLable"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.945" />

    <TextView
        android:id="@+id/distance2Checkpoint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="DistanceVal"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/distance2CheckpointLable"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.725" />

    <TextView
        android:id="@+id/speedLable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Speed:"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintStart_toStartOf="@+id/compass"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.18"
        app:layout_constraintHorizontal_bias="0.259"/>

    <TextView
        android:id="@+id/CurrPosLable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pos:"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.262"
        app:layout_constraintStart_toStartOf="@+id/compass"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.281" />

    <TextView
        android:id="@+id/HeadingLable"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:text="Heading:"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.284"
        app:layout_constraintStart_toStartOf="@+id/compass"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.377" />

    <TextView
        android:id="@+id/bearingLable"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:text="Bearing:"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.281"
        app:layout_constraintStart_toStartOf="@+id/compass"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.484" />

    <TextView
        android:id="@+id/deflectionAngleLable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deflection:"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.295"
        app:layout_constraintStart_toStartOf="@+id/compass"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.587" />

    <TextView
        android:id="@+id/distance2CheckpointLable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Distance:"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/compass"
        app:layout_constraintEnd_toEndOf="@+id/compass"
        app:layout_constraintHorizontal_bias="0.287"
        app:layout_constraintStart_toStartOf="@+id/compass"
        app:layout_constraintTop_toTopOf="@+id/compass"
        app:layout_constraintVertical_bias="0.687" />

</android.support.constraint.ConstraintLayout>